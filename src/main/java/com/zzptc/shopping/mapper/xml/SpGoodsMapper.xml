<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzptc.shopping.mapper.SpGoodsMapper">

    <select id="getAll" resultType="com.zzptc.shopping.entity.SpGoods">
        select *
        from sp_goods
        limit #{pageIndex},#{pageSize}
    </select>
</mapper>
